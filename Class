#include <iostream>
using namespace std;

class line{
    public:
      void setLength(double len);
      double getLenghth(void);
      line();
    private:
      double length;
};

line::line(void){
    cout<< "object is created" << endl;
}
void line::setlength(double len){
    length =len;
}
double line::getlength(void){
    return length;
}

int main(){
    line line;
    line.setLength(6.0);
    cout << "length of line " <<line.getlength() << endl;
